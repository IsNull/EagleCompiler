program 
	intDiv()
global
	const m:int32;
	const n:int32;
	const q:int32;
	const r:int32;
	
	proc divide(in copy const m:int32, in copy const n:int32,
				out ref var q:int32, out ref var r:int32)
	do
		q init := 0;
		r init := m;
		while r >= n do
			q := q + 1;
			r := r - n
		endwhile
	endproc

do
	debugout "m";
	debugin m init;
	debugout "n";
	debugin n init;
	call divide(m, n, q, r) init q,r;
	
	debugout "q: " & q;
	debugout "r: " & r;

	skip
endprogram